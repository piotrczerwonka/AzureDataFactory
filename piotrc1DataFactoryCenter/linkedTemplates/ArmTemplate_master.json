{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "piotrc1DataFactoryCenter"
		},
		"Blob1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'Blob1'"
		},
		"DTPiotrc1_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'DTPiotrc1'"
		},
		"DTPiotrc1_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://piotrc1.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "piotrc1DataFactoryCenter_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"Blob1_connectionString": {
						"value": "[parameters('Blob1_connectionString')]"
					},
					"DTPiotrc1_accountKey": {
						"value": "[parameters('DTPiotrc1_accountKey')]"
					},
					"DTPiotrc1_properties_typeProperties_url": {
						"value": "[parameters('DTPiotrc1_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}